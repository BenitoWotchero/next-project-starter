{
  "name": "next-project-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "templateVersion": "1.0.3",
  "scripts": {
    "setup": "node scripts/setup-project.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "validate": "node scripts/validate-project.js",
    "ai-check": "node scripts/ai-check.js",
    "release": "node scripts/release.js",
    "template:check": "node scripts/check-template-updates.js",
    "template:update": "echo 'Template update script - coming soon'",
    "template:diff": "echo 'Template diff script - coming soon'",
    "help": "echo 'Commands:\n  setup - Initial project setup\n  dev - Start development\n  validate - Check project structure\n  ai-check - Validate documentation references\n  release - Run release workflow\n  template:check - Check for template updates\n  template:update - Apply template updates\n  template:diff - Show template changes'"
  },
  "dependencies": {
    "next": "^15.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "@types/react": "^18.2.79",
    "@types/node": "^20.12.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.3.4",
    "jest": "^29.7.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "description": ""
}